<?xml version="1.0" encoding="windows-1251"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="telegony.dataaccess.common">
    <class name="telegony.dataaccess.common.ZoneType" table="zone_type" lazy="false">
        <id name="id" column="zone_type_id" type="long" access="field" unsaved-value="null">
            <generator class="assigned"/>
        </id>
        <property name="description" type="text" access="field"/>
    </class>
</hibernate-mapping>